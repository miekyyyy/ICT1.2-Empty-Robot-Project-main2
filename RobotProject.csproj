    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PlatformTarget>ARM64</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Avans.StatisticalRobot" Version="1.0.4" />
    <PackageReference Include="HiveMQtt" Version="0.24.0" />
